{
  "name": "scarcity-site",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "prebuild": "rm -rf dist",
    "build": "npm run rawbuild -- --mode=\"production\"",
    "dev": "nodemon --exec npm run build:dev --ignore dist/ --ignore coverage/",
    "build:dev": "npm run rawbuild -- --mode=\"development\"",
    "rawbuild": "webpack --config webpack.config.js",
    "postrawbuild": "cp static/* dist/"
  },
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "chance": "^1.0.18",
    "core-js": "^3.0.1",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "merge-deep": "^3.0.2",
    "nodemon": "^1.18.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/test/jest-config.js"
    ]
  },
  "devDependencies": {}
}
